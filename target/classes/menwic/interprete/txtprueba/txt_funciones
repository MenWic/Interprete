n=10;
Escribir(n);

procedimiento ciclo(){
    Mientras(n<12){
        Escribir(n);
        n = n+1;
    }
}

funcion Entero hasta10(){
    Si(n<10){
        Escribir(n);
        n = n+1;
    }
    Sino{
        Escribir("n es "+n); //VERIFICAR CONCATENACION
        retornar n;
    }
    retorna n;
}

