Entero n;

Texto t;

n =10;

t ="hola";

Escribir(n);

Escribir("ahora");

Escribir(t);

Leer(n);

Leer(t);

Si(n==10)
   Leer(n);
Sino
   Escribir("No se encontro la var");

Para(0,7)
Escribir("hola");

EnCasoDe(n)
   Caso 1:
     Escribir("caso 1");
   FinCaso
   Caso 10:
     Escribir("caso 10");
   FinCaso
FinEnCasoDe