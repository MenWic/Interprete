Entero n;
n =10;
Escribir(n);

Texto t;
t ="hola";
Escribir(t);

Leer(n);
Escribir(n);

Leer(t);
Escribir(t);

Si(n==10)
   Escribir("n es 10");
Sino
   Escribir("n no es 10");

Para(0,7)
Escribir("hola ");

n=10;
Escribir(n);

EnCasoDe(n)
   Caso 1:
     Escribir("Uno");
   FinCaso
   Caso 10:
     Escribir("Diez");
   FinCaso
   Deplano:
     Escribir("Default, deplano va");
   FinDeplano
FinEnCasoDe

Escribir(n);