Entero n;
n =0;
Escribir(n);

Texto t;
t ="hola";
Escribir(t);

Leer(n);
Escribir(n);

Leer(t);
Escribir(t);

Si(n==10){
   Escribir("n es 10");
}
Sino{
   Escribir("n no es 10");
}

Para(0,4){
Escribir("hola");
}

n=10;
Escribir(n);

EnCasoDe(n){
   Caso 1:
     Escribir("Uno");
   FinCaso
   Caso 10:
     Escribir("Diez");
   FinCaso
   Deplano:
     Escribir("Default, deplano va");
   FinCaso
}

n=10;
Escribir(n);

Mientras(n<12){
   Escribir(n);
   n = n+1;
}

Escribir(n);